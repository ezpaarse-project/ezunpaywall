{
  "swagger": "2.0",
  "info": {
    "description": "The enrichment service allows to enrich a csv or jsonl file which contains a \"doi\" field with the metadata of the ezunpaywall mirror",
    "version": "1.0.0",
    "title": "Enrich service",
    "contact": {
      "email": "ezteam@couperin.org",
      "name": "ezTeam"
    },
    "license": {
      "name": "CeCILL 2.1",
      "url": "http://www.cecill.info/licences/Licence_CeCILL_V2.1-en.html"
    }
  },
  "tags": [
    {
      "name": "enrich"
    },
    {
      "name": "ping"
    }
  ],
  "paths": {
    "/enrich/enriched": {
      "get": {
        "tags": [
          "enrich"
        ],
        "summary": "Get enriched file",
        "operationId": "get-enrich-enriched",
        "description": "Get the list of enriched files",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "list of enriched file",
            "schema": {
              "type": "array",
              "items": {
                "type": "string",
                "default": "file.csv"
              }
            }
          },
          "401": {
            "$ref": "#/responses/Not-authorized"
          }
        },
        "security": [
          {
            "x-api-key": []
          }
        ]
      }
    },
    "/enrich/uploaded": {
      "get": {
        "tags": [
          "enrich"
        ],
        "summary": "Get uploaded files",
        "operationId": "get-enriched-uploaded",
        "description": "Get the lists of uploaded files",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "list of uploaded file",
            "schema": {
              "type": "array",
              "items": {
                "type": "string",
                "default": "file.csv"
              }
            }
          },
          "401": {
            "$ref": "#/responses/Not-authorized"
          }
        },
        "security": [
          {
            "x-api-key": []
          }
        ]
      },
      "parameters": []
    },
    "/enrich/upload": {
      "post": {
        "tags": [
          "enrich"
        ],
        "summary": "Upload a file",
        "operationId": "post-enrich-upload",
        "description": "Upload a file to be enriched",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "filename of uploaded file",
            "schema": {
              "type": "string",
              "default": "id"
            }
          },
          "401": {
            "$ref": "#/responses/Not-authorized"
          }
        },
        "security": [
          {
            "x-api-key": []
          }
        ]
      },
      "parameters": []
    },
    "/enrich/enriched/${filename}": {
      "get": {
        "tags": [
          "enrich"
        ],
        "summary": "Get enriched file",
        "operationId": "get-enrich-enriched-$-filename",
        "description": "Get the enriched file, generated at the end of the enrichment process",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "filename",
            "description": "filename",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "enriched file",
            "schema": {
              "type": "file"
            }
          },
          "401": {
            "$ref": "#/responses/Not-authorized"
          }
        },
        "security": [
          {
            "x-api-key": []
          }
        ]
      }
    },
    "/enrich/job/{filename}": {
      "post": {
        "tags": [
          "enrich"
        ],
        "summary": "Enrich job",
        "operationId": "post-enrich-job-$-filename",
        "description": "Start a enrich job with uploaded file",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "filename",
            "description": "filename",
            "required": true,
            "type": "string"
          },
          {
            "in": "body",
            "name": "keys",
            "description": "apikeys",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "keys": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string"
                      },
                      "type": {
                        "type": "string"
                      },
                      "args": {
                        "type": "string"
                      },
                      "index": {
                        "type": "string"
                      },
                      "separator": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "filename of process",
            "schema": {
              "type": "string",
              "default": "id"
            }
          },
          "401": {
            "$ref": "#/responses/Not-authorized"
          }
        },
        "security": [
          {
            "x-api-key": []
          }
        ]
      }
    },
    "/enrich/states": {
      "get": {
        "tags": [
          "enrich"
        ],
        "summary": "Get list of states of enrich job",
        "operationId": "get-enrich-states",
        "description": "Get list of filenames state of enrich process",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "latest",
            "description": "latest",
            "required": false,
            "type": "boolean"
          }
        ],
        "responses": {
          "200": {
            "description": "list of states file",
            "schema": {
              "type": "array",
              "items": {
                "type": "string",
                "default": "file.json"
              }
            }
          },
          "401": {
            "$ref": "#/responses/Not-authorized"
          }
        }
      }
    },
    "/enrich/states/${filename}": {
      "get": {
        "tags": [
          "enrich"
        ],
        "summary": "Get State",
        "operationId": "get-enrich-states-$-filename",
        "description": "Get state of enrich process with his filename",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "path",
            "name": "filename",
            "description": "filename",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "list of uploaded file",
            "schema": {
              "type": "object",
              "properties": {
                "done": {
                  "type": "boolean",
                  "default": true
                },
                "loaded": {
                  "type": "integer",
                  "default": 150
                },
                "linesRead": {
                  "type": "integer",
                  "default": 3
                },
                "enrichedLines": {
                  "type": "integer",
                  "default": 3
                },
                "createdAt": {
                  "type": "string",
                  "default": "2021-11-30T10:00:00.00Z"
                },
                "endAt": {
                  "type": "string",
                  "default": "2021-11-30T10:01:00.00Z"
                },
                "error": {
                  "type": "boolean",
                  "default": false
                }
              }
            }
          },
          "401": {
            "$ref": "#/responses/Not-authorized"
          },
          "404": {
            "description": "File not found",
            "examples": {
              "File not found": {
                "message": "File not found"
              }
            }
          }
        }
      }
    },
    "/enrich": {
      "get": {
        "summary": "Name of service",
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {}
            },
            "examples": {
              "Success": {
                "message": "enrich service"
              }
            }
          }
        },
        "operationId": "get-enrich",
        "description": "Get name of enrich service",
        "tags": [
          "ping"
        ]
      },
      "parameters": []
    },
    "/enrich/ping": {
      "get": {
        "summary": "Ping enrich service",
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {}
            },
            "examples": {
              "Success": {
                "message": true
              }
            }
          }
        },
        "operationId": "get-enrich-ping",
        "description": "Ping enrich service",
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "schema": {
              "type": "object",
              "properties": {}
            },
            "x-examples": {}
          }
        ],
        "tags": [
          "ping"
        ]
      }
    },
    "/enrich/ping/redis": {
      "get": {
        "summary": "Ping redis service",
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {}
            },
            "examples": {
              "Success": {
                "message": true
              },
              "Error": {
                "message": "Cannot ping redis",
                "stackTrace": ""
              }
            }
          }
        },
        "operationId": "get-enrich-ping-redis",
        "description": "Ping redis service",
        "tags": [
          "ping"
        ]
      },
      "parameters": []
    }
  },
  "externalDocs": {
    "description": "Find out more about Swagger",
    "url": "http://swagger.io"
  },
  "definitions": {},
  "schemes": [],
  "securityDefinitions": {
    "x-api-key": {
      "name": "API Key",
      "type": "apiKey",
      "in": "header"
    }
  },
  "responses": {
    "Not-authorized": {
      "description": "Your api key does not allow access to this resource",
      "examples": {
