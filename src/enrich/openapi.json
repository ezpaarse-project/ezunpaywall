    "responses": {
      "Not-authorized": {
        "description": "Your api key does not allow access to this resource",
        "headers": {},
        "content": {
          "*/*": {
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              },
              "x-examples": {
                "example-1": {
                  "message": "Not authorized"
                }
              }
            }
          },
          "Not authorized": {
            "examples": {
              "response": {
                "value": {
                  "message": "Not authorized"
                }
              }
            }
          }
        }
      }
    },
    "securitySchemes": {
      "x-api-key": {
        "name": "API Key",
        "type": "apiKey",
        "in": "header"
      }
    }
  }
}