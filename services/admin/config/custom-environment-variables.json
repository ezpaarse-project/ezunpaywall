{ 
  "nodeEnv": "NODE_ENV",
  "timezone": "TIMEZONE",
  "apikey": "ADMIN_APIKEY",
  "healthTimeout": "HEALTH_TIMEOUT",
  "port": "PORT",
  "smtp": {
    "host": "SMTP_HOST",
    "port": "SMTP_PORT"
  },
  "notifications": {
    "sender": "NOTIFICATIONS_SENDER",
    "receivers": "NOTIFICATIONS_RECEIVERS",
    "machine": "NOTIFICATIONS_MACHINE"
  },
  "unpaywall": {
    "url": "UNPAYWALL_URL",
    "apikey": "UNPAYWALL_APIKEY"
  },
  "elasticsearch": {
    "nodes": "ELASTICSEARCH_NODES",
    "username": "ELASTICSEARCH_USERNAME",
    "password": "ELASTICSEARCH_PASSWORD",
    "maxBulkSize": "ELASTICSEARCH_MAX_BULK_SIZE",
    "indexAlias": "ELASTICSEARCH_INDEX_ALIAS",
    "timeout": "ELASTICSEARCH_TIMEOUT"
  },
  "redis":  {
    "host": "REDIS_HOST",
    "port": "REDIS_PORT",
    "password": "REDIS_PASSWORD"
  },
  "cron": {
    "downloadSnapshot": {
      "schedule": "CRON_DOWNLOAD_SNAPSHOT_SCHEDULE",
      "active": "CRON_DOWNLOAD_SNAPSHOT_ACTIVE"
    },
    "dataUpdate": {
      "schedule": "CRON_DATA_UPDATE_SCHEDULE",
      "active": "CRON_DATA_UPDATE_ACTIVE",
      "index": "CRON_DATA_UPDATE_INDEX",
      "interval": "CRON_DATA_UPDATE_INTERVAL",
      "anteriority": "CRON_DATA_UPDATE_ANTERIORITY"
    },
    "dataUpdateHistory": {
      "schedule": "CRON_DATA_UPDATE_HISTORY_SCHEDULE",
      "active": "CRON_DATA_UPDATE_HISTORY_ACTIVE",
      "index": "CRON_DATA_UPDATE_HISTORY_INDEX",
      "indexHistory": "CRON_DATA_UPDATE_HISTORY_INDEX_HISTORY",
      "interval": "CRON_DATA_UPDATE_HISTORY_INTERVAL"
    },
    "cleanFile": {
      "schedule": "CRON_CLEAN_FILE_SCHEDULE",
      "active": "CRON_CLEAN_FILE_ACTIVE",
      "changefileRetention": "CRON_CLEAN_FILE_CHANGEFILE_RETENTION",
      "reportRetention": "CRON_CLEAN_FILE_REPORT_RETENTION",
      "snapshotRetention": "CRON_CLEAN_FILE_SNAPSHOT_RETENTION",
      "accessLogRetention": "CRON_CLEAN_FILE_ACCESS_LOG_RETENTION",
      "applicationLogRetention": "CRON_CLEAN_FILE_APPLICATION_LOG_RETENTION",
      "healthcheckLogRetention": "CRON_CLEAN_FILE_HEALTHCHECK_LOG_RETENTION"
    },
    "demoApikey": {
      "schedule": "CRON_DEMO_APIKEY_SCHEDULE",
      "active": "CRON_DEMO_APIKEY_ACTIVE",
      "count": "CRON_DEMO_APIKEY_COUNT"
    }
  }
}