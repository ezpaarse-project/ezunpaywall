{ 
  "nodeEnv": "development",
  "apikey": "changeme",
  "timezone": "Europe/Paris",
  "healthTimeout": 3000,
  "port": 3002,
  "redis": {
    "host": "redis",
    "port": 6379,
    "password": "changeme"
  },
  "graphql": {
    "url": "http://graphql:3001"
  },
  "cron": {
    "cleanFile": {
      "schedule": "0 0 0 * * *",
      "active": true,
      "enrichedFileThreshold": 1,
      "uploadedFileThreshold": 1,
      "stateFileThreshold": 1,
      "accessLogThreshold": 365,
      "applicationLogThreshold": 365,
      "healthcheckLogThreshold": 30
    }
  },
  "paths": {
    "log": {
      "applicationDir": "./log/application", 
      "accessDir": "./log/access",
      "healthcheckDir": "./log/healthcheck"
    },
    "data": {
      "enrichedDir": "./data/enriched",
      "statesDir": "./data/states",
      "uploadDir": "./data/upload"
    }
  }
}